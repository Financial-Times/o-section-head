{
    "description": "A section heading to sit between the Financial Times header and the page content, indicating a section",
    "origamiType": "module",
    "origamiVersion": 1,
    "support": "https://github.com/Financial-Times/o-section-head/issues",
    "supportStatus": "active",
    "ci": {
      "travis": "https://api.travis-ci.org/repos/Financial-Times/o-section-head/builds.json"
    },
    "demosDefaults": {
      "sass": "demos/src/main.scss",
      "js": "demos/src/main.js",
      "dependencies": "o-fonts@^2.1.0"
    },
    "demos": [
      {
        "name": "standard",
        "description": "Standard section head",
        "template": "demos/src/main.mustache",
        "expanded": true,
        "documentClasses": "enhanced"
      }
    ]
}
